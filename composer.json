{
	"name": "flexsyscz/flash-messages",
	"description": "Better flash messages for you Nette web project.",
	"keywords": ["flexsyscz", "flash", "messages"],
	"homepage": "https://www.flexsys.cz",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "Václav Lukeš",
			"homepage": "https://www.vaclavlukes.cz"
		}
	],
	"require": {
		"php": ">=8.1",
		"nette/utils": "^4.0",
		"nextras/orm": "^5.0"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^2.0",
		"nette/coding-standard": "^3.3",
		"nette/bootstrap": "^3.2"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
